#!/usr/bin/env python
#coding:utf-8

import os
import sys

def main():
    script_path = os.path.realpath(__file__)
    lib_path = os.path.join(os.path.dirname(script_path),"../")
    sys.path.insert(0,os.path.realpath(lib_path))

    import silfa
    silfa.main.main()

if __name__ == "__main__":
    main()
